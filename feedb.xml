---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset>
    {% for post in site.posts limit:10 %}
    <url>
        <loc>{{ post.url | prepend: site.baseurl | prepend: site.url }}</loc>
        <lastmod>{{ post.date | date }}</lastmod>
        <changefreq>daily</changefreq>
        <priority>0.8</priority>
    </url>    
    {% endfor %}

</urlset>
